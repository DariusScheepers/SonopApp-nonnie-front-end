    this.Hk.forEach(element => {
			csvText += element.usrName + " " + element.usrSurname + "," + element.wsiFridayDinner + "," + element.wsiSaturdayBrunch + "," + element.wsiSaturdayDinner + "," + element.wsiSundayBreakfast + "," + element.wsiSundayLunch + "," + element.wsiSundayDinner + "\n";
		});
